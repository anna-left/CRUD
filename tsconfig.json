{
  "compilerOptions": {
    "lib": ["es2021"],
    "target": "es2021",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "sourceMap": true
  },
    "include": ["src"],
    "exclude": ["node_modules"]
}
